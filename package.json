{
  "name": "vue-visual",
  "version": "0.1.5",
  "description": "Vue 2 image and video loader supporting lazy loading, background videos, fixed aspect ratios, low rez poster images, transitions, loaders, slotted content and more and more",
  "main": "index.js",
  "scripts": {
    "test": "yarn test:e2e",
    "build": "node_modules/.bin/webpack && node_modules/.bin/webpack -p",
    "version": "npm run build && git add -A",
    "postversion": "git push --follow-tags",
    "test:e2e": "node test/e2e/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BKWLD/vue-visual.git"
  },
  "keywords": [
    "vue",
    "lazy-load",
    "cover-video"
  ],
  "author": "Bukwild",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BKWLD/vue-visual/issues"
  },
  "homepage": "https://github.com/BKWLD/vue-visual#readme",
  "dependencies": {
    "is-numeric": "^0.0.5",
    "lodash": "3 - 4",
    "scrollmonitor": "^1.1.3"
  },
  "devDependencies": {
    "apply-loader": "^0.1.0",
    "chromedriver": "^2.25.1",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.11.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.0",
    "express": "^4.14.0",
    "express-urlrewrite": "^1.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "nightwatch": "^0.9.9",
    "selenium-server": "^2.53.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "vue": "^2.1.0",
    "vue-loader": "^10.0.1",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4"
  }
