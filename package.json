{
  "name": "vue-visual",
  "version": "0.7.3",
  "description": "Vue 2 image and video loader supporting lazy loading, background videos, fixed aspect ratios, low rez poster images, transitions, loaders, slotted content and more and more",
  "main": "index.js",
  "scripts": {
    "serve": "PORT=3000 node examples/server",
    "test": "yarn test:e2e",
    "test:e2e": "node test/e2e/runner.js",
    "build": "webpack",
    "version": "npm run build && git add -A",
    "postversion": "git push --follow-tags && yarn deploy-storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BKWLD/vue-visual.git"
  },
  "keywords": [
    "vue",
    "lazy-load",
    "cover-video"
  ],
  "author": "Bukwild",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BKWLD/vue-visual/issues"
  },
  "homepage": "https://github.com/BKWLD/vue-visual#readme",
  "dependencies": {
    "is-numeric": "^0.0.5",
    "lodash": ">= 4.17.11",
    "scrollmonitor": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@storybook/addon-actions": "^5.0.6",
