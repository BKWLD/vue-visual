    "cross-spawn": "^5.0.1",
    "css-loader": "^2.1.1",
    "express": "^4.14.0",
    "express-urlrewrite": "^1.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nightwatch": "^0.9.9",
