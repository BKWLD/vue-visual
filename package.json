    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/vue": "^5.0.6",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "chromedriver": "^2.35",
    "coffee-loader": "^0.9.0",
